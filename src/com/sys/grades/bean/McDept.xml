<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sys.grades.bean">

	<class name="McDept" table="MC_DEPT">
		<composite-id>
			<key-property name="collegename" column="COLLEGE_NAME" type="string" />
			<key-property name="majornum" column="MAJOR_NUMBER" type="string" />
		</composite-id>
		<property name="majorname" column="MAJOR_NAME" type="java.lang.String" />
	</class>

</hibernate-mapping>